import java.util.Scanner;
 
public class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int a, b, c;
        String s;
        a = in.nextInt();
        b = in.nextInt();
        System.out.println((a * b) % 2 == 1 ? "Odd" : "Even");
        in.close();
    }
}
